let template =
        ./Name/template sha256:e7850d1d46f67aebfd3f46158ee2574a892e72be1c4b190618f162145ebea549
      ? ./Name/template

let NameTemplate =
        ../../Internal/NameTemplate/Folder/Type sha256:4b5ee9941c2398c2749b45d9324fdb122c8a149bd2fe4b17305a550c9559b55d
      ? ../../Internal/NameTemplate/Folder/Type

let Predicate =
        ../../Internal/Predicate/Type sha256:39e210b82052eda7f6cd4814c3d907c727b2ebea1c3c135957eef943014b46e9
      ? ../../Internal/Predicate/Type

let text =
        ./Name/Component/text sha256:fd8cb105dcdb6f82d71749f8b291eb66df0d65e3ae67046f481d853616559777
      ? ./Name/Component/text

in  λ(name : Text) →
    λ(predicate : Predicate) →
    λ(Folder : Type) →
    λ ( Fix
      : { nameF : NameTemplate
        , predicateF : Predicate
        , subfoldersF : List Folder
        } →
          Folder
      ) →
      Fix
        { nameF = template [ text name ]
        , predicateF = predicate
        , subfoldersF = [] : List Folder
        }
