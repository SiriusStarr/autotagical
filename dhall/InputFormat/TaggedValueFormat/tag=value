      ? ../../Internal/InputFormat/Separator

let tagsBeforeValues =
        ./tagsBeforeValues sha256:6eefebd6d8d480f918016e8a69060e0b826f3fee336cdcc18e3e655d6f95f18d
      ? ./tagsBeforeValues

in  λ(pureTagsAllowed : Bool) →
      tagsBeforeValues
        (None Separator)
        (Separator.Keyword "=")
