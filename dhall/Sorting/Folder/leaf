let template =
        ./Name/template sha256:a62aa3a4904c515d04992affc32eddd8e2ffbbca8e2a35996a9934b75b125a10
      ? ./Name/template

let NameTemplate =
        ../../Internal/NameTemplate/Folder/Type sha256:64c9b5b4460cf305506169a8bfe165370816c6f6496bd4c3eb7234e9789f28d1
      ? ../../Internal/NameTemplate/Folder/Type

let Predicate =
        ../../Internal/Predicate/Type sha256:7bbd5275d9b92c0c5b1c7c0f05885b85796a14c4813d02aae7148a9d418deedf
      ? ../../Internal/Predicate/Type

let text =
        ./Name/Component/text sha256:19ad54d7a89844f8079eae75391c08737252663f82d22c7c33f407989e65f56c
      ? ./Name/Component/text

in  λ(name : Text) →
    λ(predicate : Predicate) →
    λ(Folder : Type) →
    λ ( Fix
      : { nameF : NameTemplate
        , predicateF : Predicate
        , subfoldersF : List Folder
        } →
          Folder
      ) →
      Fix
        { nameF = template [ text name ]
        , predicateF = predicate
        , subfoldersF = [] : List Folder
        }
